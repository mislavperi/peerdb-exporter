# PeerDB Connection Configuration
# Copy this file to .env and fill in your actual values

# Database host (required)
PEERDB_HOST=localhost

# Database port (required)
PEERDB_PORT=5432

# Database username (required)
PEERDB_USERNAME=peerdb_user

# Database password (required)
PEERDB_PASSWORD=your_password_here

# Database name (optional, defaults to peerdb_stats)
PEERDB_DATABASE=peerdb_stats